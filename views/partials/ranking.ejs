<div id="ranking">
  <div class="uk-flex uk-flex-center uk-margin-top uk-heading-line">
    <h1 class="uk-heading-medium">Classement</h1>
  </div>
  <% if(host) { %>
    <table id="rankedPlayer"></table>
    <p class="uk-text-center uk-text-large">Le tableau des scores s'affichera dans <strong id="rankingTimer">10</strong> secondes !</p>
    <% } else { %>
      <h1>Merci pour votre participation</h1>
      <p class="uk-text-lead">Le classement ainsi que votre score sont actuellement affichés sur l'écran principal</p>
  <% } %>
</div>
